  "scripts": {
    "start": "react-scripts start",
    "start:storybook": "start-storybook --port 6006 --static-dir public",
    "build": "react-scripts build",
    "build:storybook": "build-storybook --static-dir public",
    "test": "react-scripts test",
    "test:integration": "jest -c integration/jest.config.js",
    "test:integrationWithWatch": "jest -c integration/jest.config.js --watch",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "extract-intl": "node ./internals/scripts/extract-intl.js",
    "plop": "plop",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "globals": {
      "page": true,
      "browser": true,
      "context": true,
      "jestPuppeteer": true
    },
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
      "jsx-a11y"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
